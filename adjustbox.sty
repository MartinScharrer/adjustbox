%
% \begin{macro}{\marginbox@}
% (1) Parse margins (must be done here to allow the user to use the original box dimensions).
% (2) Change box margins and (3) place the box.
%    \begin{macrocode}
\def\marginbox@#1{%
    \adjbox@parse@vp#1 \\%
    \@marginbox
        \adjbox@vllx
        \adjbox@vlly
        \adjbox@vurx
        \adjbox@vury
        \collectedbox
    \box\collectedbox
}
%    \end{macrocode}
% \end{macro}
%
%
% \begin{macro}{\@marginbox}
% Adds the given margins to the depth, width and height.
% The left margin is created by an horizontal skip.
% This implementation assumes that the margins are positive and no special checks are added.
% While negative margins will trim some margin off, this will not lead to correct results if this amounts
% are larger than the existing dimensions. For this the \Macro\trimbox macro should be used.
%    \begin{macrocode}
\def\@marginbox#1#2#3#4#5{%
    \setbox#5=\hbox{%
        \adjbox@setdim\@tempdima{#1}%
        \hskip\@tempdima
        \adjbox@addsetdim{\dp#5}{\dp#5}{#2}%
        \adjbox@addsetdim{\wd#5}{\wd#5}{#3}%
        \adjbox@addsetdim{\ht#5}{\ht#5}{#4}%
        \box#5%
    }%
}
%    \end{macrocode}
% \end{macro}
%
%
% \begin{macro}{\adjbox@addsetdim@etex}
%    \begin{macrocode}
\def\adjbox@addsetdim@etex#1#2#3{%
    #1=\dimexpr#2 + #3\relax
}
%    \end{macrocode}
% \end{macro}
%
%
% \begin{macro}{\adjbox@addsetdim@tex}
%    \begin{macrocode}
\def\adjbox@addsetdim@tex#1#2#3{%
    \@tempdima=#2\relax
    \advance\@tempdima by #2\relax
    #1=\@tempdima
}
%    \end{macrocode}
% \end{macro}
%
%
% \begin{macro}{\adjbox@addsetdim@latex}
%    \begin{macrocode}
\def\adjbox@addsetdim@latex#1#2#3{%
    \setlength\@tempdima{#2}%
    \addtolength\@tempdima{#3}%
    \setlength#1{\@tempdima}%
}
%    \end{macrocode}
% \end{macro}
%
%
% \begin{macro}{\adjbox@addsetdim@pgf}
%    \begin{macrocode}
\def\adjbox@addsetdim@pgf#1#2#3{%
    \pgfmathsetlength\@tempdima{#2 + #3}%
    #1=\@tempdima
}
%    \end{macrocode}
% \end{macro}
%
%
% \begin{macro}{\adjbox@setdim@etex}
%    \begin{macrocode}
\def\adjbox@setdim@etex#1#2{%
    #1=\dimexpr#2\relax
}
%    \end{macrocode}
% \end{macro}
%
%
% \begin{macro}{\adjbox@setdim@tex}
%    \begin{macrocode}
\def\adjbox@setdim@tex#1#2{%
    #1=#2\relax
}
%    \end{macrocode}
% \end{macro}
%
%
% \begin{macro}{\adjbox@setdim@latex}
%    \begin{macrocode}
\def\adjbox@setdim@latex#1#2{%
    \setlength{#1}{#2}%
}
%    \end{macrocode}
% \end{macro}
%
%
% \begin{macro}{\adjbox@setdim@pgf}
%    \begin{macrocode}
\def\adjbox@setdim@pgf#1#2{%
    \pgfmathsetlength{#1}{#2}%
}
%    \end{macrocode}
% \end{macro}
%
%
%    \begin{macrocode}
\begingroup
\expandafter\ifx\csname eTeXversion\endcsname\relax
    \global\let\adjbox@addsetdim\adjbox@addsetdim@etex
    \global\let\adjbox@setdim\adjbox@setdim@etex
\else
    \global\let\adjbox@addsetdim\adjbox@addsetdim@latex
    \global\let\adjbox@setdim\adjbox@setdim@latex
\fi
\endgroup
%    \end{macrocode}


