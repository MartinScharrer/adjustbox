% \begin{macro}{\@lapbox}
%    \begin{macrocode}
\def\@lapbox#1#2{%
    \@collectbox{\@@lapbox{#2}}%
}
%    \end{macrocode}
% \end{macro}
%
%
% \begin{macro}{\@@lapbox}
%    \begin{macrocode}
\def\@@lapbox#1{%
    \setlength\@tempdima{#1}%
    \@tempdimb\width
    \advance\@tempdimb by \ifdim\@tempdima>\z@-\fi\@tempdima
    \hb@xt@\@tempdimb{%
        \ifdim\@tempdima<\z@
            \hss
        \fi
        \BOXCONTENT
        \ifdim\@tempdima>\z@
            \hss
        \fi
    }%
}
%    \end{macrocode}
% \end{macro}
%
%
% \begin{macro}{\i@lapbox}
%    \begin{macrocode}
\def\i@lapbox#1#2{%
    \@collectbox{\i@@lapbox{#1}{#2}}%
}
%    \end{macrocode}
% \end{macro}
%
%
% \begin{macro}{\i@@lapbox}
%    \begin{macrocode}
\def\i@@lapbox#1#2{%
    \setlength\@tempdima{#2}%
    \setlength\@tempdimb{#1}%
    \hb@xt@\@tempdimb{%
        \ifdim\@tempdima<\z@
            \hss
            \hb@xt@-\@tempdima{\BOXCONTENT\hss}%
            \hskip\@tempdimb
        \else
            \hskip\@tempdimb
            \hb@xt@\@tempdima{\hss\BOXCONTENT}%
            \hss
        \fi
    }%
}
%    \end{macrocode}
% \end{macro}
