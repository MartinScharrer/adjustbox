%    \begin{macrocode}
\newcommand\marginbox[1]{%
    \@collectbox{\marginbox@{#1}}%
}
%    \end{macrocode}
% \end{macro}
%
%
% \begin{macro}{\marginbox@}
% (1) Parse margins (must be done here to allow the user to use the original box dimensions).
% (2) Change box margins and (3) place the box.
%    \begin{macrocode}
\def\marginbox@#1{%
    \adjbox@parse@vp#1 \\%
    \@marginbox
        \adjbox@vllx
        \adjbox@vlly
        \adjbox@vurx
        \adjbox@vury
        \collectedbox
    \box\collectedbox
}
%    \end{macrocode}
% \end{macro}
%
%
% \begin{macro}{\@marginbox}
% Adds the given margins to the depth, width and height.
% The left margin is created by an horizontal skip.
% This implementation assumes that the margins are positive and no special checks are added.
% While negative margins will trim some margin off, this will not lead to correct results if this amounts
% are larger than the existing dimensions. For this the \Macro\trimbox macro should be used.
%    \begin{macrocode}
\def\@marginbox#1#2#3#4#5{%
    \setbox#5=\hbox{%
        \adjbox@setdim\@tempdima{#1}%
        \hskip\@tempdima
        \adjbox@setadddim{\@tempdima}{\dp#5}{#2}%
        \adjbox@setadddim{\@tempdimb}{\wd#5}{#3}%
        \adjbox@setadddim{\@tempdimc}{\ht#5}{#4}%
        \dp#5=\@tempdima
        \wd#5=\@tempdimb
        \ht#5=\@tempdimc
        \box#5%
    }%
}
%    \end{macrocode}
% \end{macro}
%
%
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% \subsection{Dimension macros}
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%
% \begin{macro}{\adjbox@setadddim@etex}
%    \begin{macrocode}
\def\adjbox@setadddim@etex#1#2#3{%
    #1=\dimexpr#2 + #3\relax
}
%    \end{macrocode}
% \end{macro}
%
%
% \begin{macro}{\adjbox@setadddim@tex}
%    \begin{macrocode}
\def\adjbox@setadddim@tex#1#2#3{%
    \@tempdima=#2\relax
    \advance\@tempdima by #2\relax
    #1=\@tempdima
}
%    \end{macrocode}
% \end{macro}
%
%
% \begin{macro}{\adjbox@setadddim@latex}
%    \begin{macrocode}
\def\adjbox@setadddim@latex#1#2#3{%
    \setlength\@tempdima{#2}%
    \addtolength\@tempdima{#3}%
    \setlength#1{\@tempdima}%
}
%    \end{macrocode}
% \end{macro}
%
%
% \begin{macro}{\adjbox@setadddim@pgf}
%    \begin{macrocode}
\def\adjbox@setadddim@pgf#1#2#3{%
    \pgfmathsetlength\@tempdima{#2 + #3}%
    #1=\@tempdima
}
%    \end{macrocode}
% \end{macro}
%
%
% \begin{macro}{\adjbox@setsubdim@etex}
%    \begin{macrocode}
\def\adjbox@setsubdim@etex#1#2#3{%
    #1=\dimexpr#2 -\dimexpr#3\relax\relax
}
%    \end{macrocode}
% \end{macro}
%
%
% \begin{macro}{\adjbox@setsubdim@tex}
%    \begin{macrocode}
\def\adjbox@setsubdim@tex#1#2#3{%
    \@tempdima=#2\relax
    \advance\@tempdima by -#2\relax
    #1=\@tempdima
}
%    \end{macrocode}
% \end{macro}
%
%
% \begin{macro}{\adjbox@setsubdim@latex}
%    \begin{macrocode}
\def\adjbox@setsubdim@latex#1#2#3{%
    \setlength\@tempdima{#2}%
    \@tempdima=-\@tempdima
    \addtolength\@tempdima{#3}%
    \setlength#1{-\@tempdima}%
}
%    \end{macrocode}
% \end{macro}
%
%
% \begin{macro}{\adjbox@setsubdim@pgf}
%    \begin{macrocode}
\def\adjbox@setsubdim@pgf#1#2#3{%
    \pgfmathsetlength\@tempdima{#2 - (#3)}%
    #1=\@tempdima
}
%    \end{macrocode}
% \end{macro}
%
%
% \begin{macro}{\adjbox@setdim@etex}
%    \begin{macrocode}
\def\adjbox@setdim@etex#1#2{%
    #1=\dimexpr#2\relax
}
%    \end{macrocode}
% \end{macro}
%
%
% \begin{macro}{\adjbox@setdim@tex}
%    \begin{macrocode}
\def\adjbox@setdim@tex#1#2{%
    #1=#2\relax
}
%    \end{macrocode}
% \end{macro}
%
%
% \begin{macro}{\adjbox@setdim@latex}
%    \begin{macrocode}
\def\adjbox@setdim@latex#1#2{%
    \setlength{#1}{#2}%
}
%    \end{macrocode}
% \end{macro}
%
%
% \begin{macro}{\adjbox@setdim@pgf}
%    \begin{macrocode}
\def\adjbox@setdim@pgf#1#2{%
    \pgfmathsetlength{#1}{#2}%
}
%    \end{macrocode}
% \end{macro}
%
%
%    \begin{macrocode}
\begingroup
\expandafter\ifx\csname eTeXversion\endcsname\relax
    \global\let\adjbox@setadddim\adjbox@setadddim@latex
    \global\let\adjbox@setsubdim\adjbox@setsubdim@latex
    \global\let\adjbox@setdim\adjbox@setdim@latex
\else
    \global\let\adjbox@setadddim\adjbox@setadddim@etex
    \global\let\adjbox@setsubdim\adjbox@setsubdim@etex
    \global\let\adjbox@setdim\adjbox@setdim@etex
\fi
\endgroup
%    \end{macrocode}
