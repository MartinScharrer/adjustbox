% \begin{macro}{\@clipbox}
%    \begin{macrocode}
\def\@clipbox#1#2#3#4#5{%
    \@trimbox{#1}{#2}{#3}{#4}{#5}%
    \setbox#5=\hbox{%
        \adjbox@settobp\WIDTH{\wd#5}%
        \adjbox@settobp\HEIGHT{\ht#5}%
        \adjbox@settobp\DEPTH{\dp#5}%
        \special{pdf:content q }%
            \special{%
                pdf:literal direct
                  0 0 m 0 \HEIGHT\space l \WIDTH\space \HEIGHT\space l
                  \WIDTH\space -\DEPTH\space l 0 -\DEPTH\space l 0 0 l
              }%
            \special{pdf:literal direct W }%
            \special{pdf:literal direct n }%
              \special{pdf:literal direct -1 0 0 -1 0 0 cm }%
                \special{pdf:content q }%
                  \special{pdf:literal direct -1 0 0 -1 0 0 cm }%
                    \hbox{\box#5}%
                \special{pdf:literal direct Q }%
        \special{pdf:literal direct Q }%
    }%
}
%    \end{macrocode}
